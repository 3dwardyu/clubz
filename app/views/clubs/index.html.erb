<h1>Super Exclusive Clubs</h1>

<% if @clubs.count > 0 %>
  <% @clubs.each do |club| %>
    <h3>
      <% if current_user %>
        <%= link_to club.name, club_path(club) %>
      <% else %>
        <%= club.name %>
      <% end %>
    </h3>
    <p><%= club.description %></p>
  <% end %>
<% else %>
  <p>No clubs found.</p>
<% end %>